<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cloud.gds.cleaning.mapper.DataFieldMapper">

	<!-- 可根据自己的需求，是否要使用 -->
	<resultMap id="baseMap" type="com.cloud.gds.cleaning.api.entity.DataField">
		<result column="id" property="id"/>
		<result column="name" property="name"/>
		<result column="rule_id" property="ruleId"/>
	</resultMap>

	<update id="reduction">
		update dataclean_data_field
		set
		is_deleted = 0
		where is_deleted = 1
		and id = #{id}
	</update>


</mapper>
